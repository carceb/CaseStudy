@model IEnumerable<CaseStudy.Models.Participant>

@{
    ViewBag.Title = "Participants";
}

<h2>Participants</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.ParticipantName)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.ParticipantName)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.ParticipantID }) |
            @Html.ActionLink("Details", "Details", new { id=item.ParticipantID }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.ParticipantID })
        </td>
    </tr>
}

</table>
